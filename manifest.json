{
	"name": "蘇れIE",
	"manifest_version": 3,
	"version": "1.0.0",
	"action":{
    "default_icon": {
      "16": "icon.png"
    },
    "default_title":"蘇れIE",
    "default_popup":"popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ],
  "host_permissions": [
    "https://twitter.com/*"
  ],
  "web_accessible_resources": [{
    "resources": ["*.png"],
    "matches": ["https://twitter.com/*"]
  }]
}
