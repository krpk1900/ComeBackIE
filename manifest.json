{
	"name": "蘇れIE",
	"manifest_version": 2,
	"version": "1.0.0",
	"browser_action":{
    "default_icon": {
      "16": "icon.png"
    },
    "default_title":"蘇れIE",
    "default_popup":"popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ],
  "permissions": [
    "tabs",
    "https://twitter.com/*"
  ],
  "web_accessible_resources": [
    "*.png"
  ]
}
